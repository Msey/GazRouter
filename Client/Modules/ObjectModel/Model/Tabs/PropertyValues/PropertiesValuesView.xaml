<UserControl x:Class="GazRouter.ObjectModel.Model.Tabs.PropertyValues.PropertiesValuesView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" 
             d:DesignHeight="300"
             xmlns:propertyValues="clr-namespace:GazRouter.ObjectModel.Model.Tabs.PropertyValues"
             xmlns:measurings="clr-namespace:GazRouter.Controls.Dialogs.ObjectDetails.Measurings;assembly=Controls"
             d:DesignWidth="400" 
             x:Name="PropertiesValuesControl" 
             d:DataContext="{d:DesignInstance propertyValues:PropertiesValuesViewModel}">
    
    
    <Grid>
        <measurings:MeasuringsView DataContext="{Binding Measurings}"/>
    </Grid>
    
</UserControl>