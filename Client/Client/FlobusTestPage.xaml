<UserControl x:Class="GazRouter.Client.FlobusTestPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:flobus="clr-namespace:GazRouter.Flobus;assembly=Flobus"
    xmlns:thumbnail="clr-namespace:GazRouter.Flobus.Thumbnail;assembly=Flobus"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">
    
    <Grid x:Name="LayoutRoot" Background="White">
        <flobus:Schema x:Name="Schema" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"  IsReadOnly="False"/>
      <thumbnail:SchemaNaviagationPane IsExpanded="True" Schema="{Binding ElementName=Schema}" HorizontalAlignment="Left" VerticalAlignment="Bottom" />
    </Grid>
</UserControl>
